<div>
    <div>
        <div class="container">
            <div class="row center">
                <h5 class="header col s12 light">Search Results</h5>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col s4 offset-s1 teal">photo</div>
        <div class="col s6 blue">
            <p>placeholder</p>
            {{#each plants}}
            <h1>Plant Name: {{Common-Name}}</h1>
            <h2>Scientific Name: {{Scientific-Name}}</h2>
            <p>Sun Needs: {{Sunlight-Needs}}</p>
            <p>Soil Moisture {{Moisture-Levels}}</p>
            <p>Is it toxic? {{Toxic}}</p>
            <p>Maintenance Level: {{Maintenance-Level}}</p>
            <p>Price Range: {{Affordability}}</p>
            {{/each}}
        </div>
    </div>
    <div class="row">
        <div class="col s4 offset-s1 teal">image</div>
        <div class="col s6 blue">info</div>
    </div>
</div>

<div class="container">
    <div class="section">

        <div class="row">
            <div class="col s12 center">
                <h3><i class="mdi-content-send brown-text"></i></h3>
                <h4>Start a New Search</h4>
                <div class="row center">
                    <a href="./search" id="download-button"
                        class="btn-large waves-effect waves-light teal lighten-1">Search</a>
                </div>
            </div>
        </div>

    </div>
</div>



<footer class="page-footer teal">
